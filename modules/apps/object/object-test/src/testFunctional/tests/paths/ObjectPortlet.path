<html>
<head>
<title>ObjectPortlet</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">ObjectPortlet</td></tr>
</thead>

<tbody>
	<tr>
		<td>ANY_PAGE</td>
		<td>//ul[contains(@class,'pagination')]//li[contains(@class,'page-item')]//*[contains(@class,'page-link')][normalize-space(text())='${key_pageNumber}']</td>
		<td></td>
	</tr>
	<tr>
		<td>ARROW_DOWN_ICON</td>
		<td>//button[contains(@class,'dropdown-toggle btn btn-monospaced btn-outline-borderless btn-outline-secondary')]</td>
		<td></td>
	</tr>
	<tr>
		<td>CHECK_UNCHECK_DROPDOWN_ITEM</td>
		<td>//button[contains(@class,'dropdown-item')][contains(text(),'${key_dropdowntItem}')]</td>
		<td></td>
	</tr>
	<tr>
		<td>ENTRY_COLUMN</td>
		<td>//div[contains(@class,'dnd-th')]//div[contains(text(),'${key_column}')]</td>
		<td></td>
	</tr>
	<tr>
		<td>ENTRY_VALUE</td>
		<td>//div[contains(@class,'dnd-tbody')]//div[contains(text(),'${key_entry}')]</td>
		<td></td>
	</tr>
	<tr>
		<td>ENTRY_VALUE_SPECIFIC_ORDER</td>
		<td>//div[${resultOrder}]//div[contains(@class,'dnd-tbody')]//div[contains(text(),'${key_entry}')]</td>
		<td></td>
	</tr>
	<tr>
		<td>ENTRY_KEBAB_MENU</td>
		<td>//div[contains(@class,'dnd-tbody')]//div[contains(text(),'${key_entry}')]/following-sibling::div//button</td>
		<td></td>
	</tr>
	<tr>
		<td>DELETE_ENTRY_BUTTON</td>
		<td>//div[contains(@class,'show')]//button[contains(.,'Delete')]</td>
		<td></td>
	</tr>
	<tr>
		<td>PAGINATION_ITEMS_PER_PAGE_SELECT</td>
		<td>//div[contains(@class,'pagination-items-per-page')]/button[contains(@class,'dropdown-toggle')]</td>
		<td></td>
	</tr>
	<tr>
		<td>SEARCH_FIELD</td>
		<td>//input[contains(@aria-label,'Search')]</td>
		<td></td>
	</tr>
	<tr>
		<td>SELECT_CUSTOM_OBJECT</td>
		<td>//div[(@class='table-list-title') and contains (.,'${key_label}')]</td>
		<td></td>
	</tr>
	<tr>
	<td>SORT_FIELD</td>
	<td>//button[@class='btn-sorting inline-item text-nowrap text-truncate-inline btn btn-sm btn-unstyled' and contains(text(), '${key_sortFieldName}')]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>